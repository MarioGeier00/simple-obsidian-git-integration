{
  "name": "git-integration-plugin",
  "version": "0.4.0",
  "description": "This is an Obsidian plugin to enable Git capabilities such as initialise a git repository and make git commits.",
  "main": "main.js",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [],
  "author": "noradroid",
  "license": "MIT",
  "devDependencies": {
    "@stylistic/eslint-plugin": "^1.6.1",
    "@stylistic/eslint-plugin-ts": "^1.6.1",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "builtin-modules": "3.3.0",
    "esbuild": "^0.18.20",
    "esbuild-plugin-eslint": "^0.3.7",
    "eslint-config-prettier": "^9.1.0",
    "obsidian": "latest",
    "prettier": "^3.2.5",
    "tslib": "2.4.0",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "simple-git": "^3.22.0"
  }
}
